{
  "redirects": [
    {
      "source": "/shop",
      "destination": "/products",
      "permanent": true
    },
    {
      "source": "/shop/:path*",
      "destination": "/products/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "dibrunolab.com"
        }
      ],
      "destination": "https://www.dibrunolab.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}

