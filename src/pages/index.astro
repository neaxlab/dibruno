---
import Layout from '@/layouts/Layout.astro';
import Hero from '@/components/home/hero.astro';
import BetSellers from '@/components/home/betSellers.astro';
import IngredientsResults from '@/components/home/ingredientsResults.astro';
import Testimonials from '@/components/home/testimonials.astro';
import Instructions from '@/components/home/Instructions.astro';
import ProductCarousel from '@/components/home/ProductCarousel.astro';
import ChatWithUS from '@/components/faqs/chatWithUS.astro';
import { getCurrentCanonicalUrl } from '@/utils/seo';
// Welcome to Astro! Wondering what to do next? Check out the Astro documentation at https://docs.astro.build
// Don't want to use any of this? Delete everything in this file, the `assets`, `components`, and `layouts` directories, and start fresh.
export const prerender = false;

const canonicalUrl = getCurrentCanonicalUrl(Astro.url.pathname);
---

<Layout canonicalUrl={canonicalUrl}>
	<Hero />
	<BetSellers />
	<IngredientsResults />
	<ProductCarousel />
	<Instructions />
	<ChatWithUS margin={true}/>
	<Testimonials />
</Layout>
